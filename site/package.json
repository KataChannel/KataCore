{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "// ========== DEVELOPMENT ==========": "",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:https": "next dev --experimental-https",
    "dev:port": "next dev -p",
    "// ========== BUILD & START ==========": "",
    "build": "next build",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:turbo": "next build --experimental-build-worker",
    "build:optimized": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next build",
    "build:standalone": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "build:check": "npm run type-check && npm run lint && npm run build",
    "start": "next start",
    "start:fast": "NODE_OPTIONS='--max-old-space-size=2048' NEXT_TELEMETRY_DISABLED=1 next start",
    "start:optimized": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=2048 --max-semi-space-size=1024' next start",
    "start:turbo": "next start --turbo",
    "start:prod": "NODE_ENV=production next start",
    "start:standalone": "node .next/standalone/server.js",
    "start:port": "next start -p",
    "start:benchmark": "time NODE_OPTIONS='--max-old-space-size=2048' NEXT_TELEMETRY_DISABLED=1 next start",
    "start:pwa": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=2048' next start",
    "// ========== TESTING ==========": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=src/.*\\.integration\\.(ts|tsx)$",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "// ========== LINTING & FORMATTING ==========": "",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "lint:report": "next lint --format html --output-file eslint-report.html",
    "format": "prettier --write .",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "// ========== DATABASE ==========": "",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed/unified-comprehensive-seed.ts",
    "db:seed:master": "tsx prisma/seed/master-seed.ts",
    "db:seed:unified": "tsx prisma/seed/unified-comprehensive-seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:pull": "prisma db pull",
    "db:format": "prisma format",
    "// ========== ADMIN TOOLS ==========": "",
    "admin:create": "tsx scripts/quick-create-admin.ts",
    "admin:create-interactive": "tsx scripts/create-admin-user.ts",
    "admin:super": "tsx scripts/create-super-admin.ts",
    "admin:cli": "node scripts/super-admin-cli.js",
    "admin:list": "node scripts/super-admin-cli.js list",
    "admin:check": "node scripts/super-admin-cli.js check",
    "// ========== UTILITIES ==========": "",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules .turbo tsconfig.tsbuildinfo",
    "clean:cache": "rm -rf .next/cache node_modules/.cache",
    "// ========== SECURITY & MONITORING ==========": "",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:scan": "npm audit --audit-level moderate",
    "health": "curl -f http://localhost:3000/api/health || echo 'Site not healthy'",
    "sitemap": "next-sitemap",
    "// ========== DEPLOYMENT ==========": "",
    "export": "next export",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "// ========== WORKFLOW ==========": "",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prebuild": "npm run db:generate",
    "analyze": "ANALYZE=true next build",
    "ci": "npm run lint && npm run type-check && npm run test:ci && npm run build",
    "dev:full": "npm run db:generate && npm run dev",
    "fresh": "npm run clean:all && npm install && npm run db:generate && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-data-grid": "^8.5.2",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "10.4.20",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "jssip": "^3.10.1",
    "lucide-react": "^0.517.0",
    "nanoid": "^5.1.5",
    "next": "latest",
    "next-auth": "5.0.0-beta.25",
    "next-pwa": "^5.6.0",
    "postcss": "^8.5.1",
    "postgres": "^3.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "typescript": "5.7.3",
    "use-debounce": "^10.0.4",
    "zod": "^3.25.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.18",
    "@types/node": "^22.16.3",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  }
}
